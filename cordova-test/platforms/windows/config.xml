<?xml version='1.0' encoding='utf-8'?>
<widget id="tw.at.yescirculation.mediadotpaper" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MediaDotPaper</name>
    <description>
        My first cordova App to help people to add some extra functions to their media.
    </description>
    <author email="yescirculation@gmail.com" href="http://yescirculation.at.tw">
        Yes Circulation
    </author>
    <content src="index.html" />
    <access origin="*" />
    <hook src="scripts/prepareAngular6App.js" type="before_prepare" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="blob:*" />
</widget>
